{
  "name": "voronoi/apprentice",
  "description": "Laravel package for the apprentice iOS app",
  "license": "MIT",
  "authors": [
    {
      "name": "Tim Baker",
      "email": "timbone945@gmail.com"
    }
  ],
  "version": "1.0.0",
  "require": {
    "php": ">=7.2",
    "illuminate/support": ">=7.2",
    "endroid/qr-code": "^3.7"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.0",
    "orchestra/testbench": "~5.2.0"
  },
  "suggest": {
    "roave/security-advisories": "Avoids installation of package versions with vulnerabilities",
    "sensiolabs/security-checker": "Checks your composer.lock for vulnerabilities"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "Voronoi\\Apprentice\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Voronoi\\Apprentice\\ServiceProvider"
      ],
      "aliases": {
        "Apprentice": "Voronoi\\Apprentice\\Facades\\Apprentice"
      }
    }
  }
}
